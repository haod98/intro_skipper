{
  "name": "Intro Skipper",
  "description": "Skip intros",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting", "tabs"],
  "action": {
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  }
}
